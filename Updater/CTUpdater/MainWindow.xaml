	Title="Cinematic Tools Updater"
	Height="138.07"
	Width="412.587"
	ResizeMode="NoResize"
	WindowStyle="ToolWindow">
	<Grid
		Margin="0,0,-1,0"
		Background="#FF000000">
		<Label
			Name="statusLabel"
			Content="Checking version file"
			FontFamily="./#Purista Medium"
			FontSize="16"
			HorizontalContentAlignment="Center"
			VerticalAlignment="Top"
			Foreground="#FFFFFFFF"
			Margin="0,59,0,0"
			Background="{x:Null}" />
		<Image
			HorizontalAlignment="Left"
			Margin="10,10,0,0"
			VerticalAlignment="Top"
			Width="383"
			Source="title.png" />
	</Grid>
</Window>
